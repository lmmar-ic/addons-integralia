<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sale_order_udo_without_prices_report" model="ir.actions.report">
            <field name="name">Presupuesto Udo sin Precios</field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="paperformat_id">4</field>
            <field name="report_name">sale_udo_without_prices.report_sale_order_udo_without_prices</field>
